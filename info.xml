<?xml version="1.0"?>
<extension key="com.octopus8.whatsapp" type="module">
  <file>whatsapp</file>
  <name>Octopus8 WhatsApp</name>
  <description>This extension provides WhatsApp message integration</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Asliddin Oripov</author>
    <email>asliddin@octopus8.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://civicrm.org</url>
    <url desc="Documentation">https://github.com/octogeek/com.octopus8.whatsapp</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2021-11-21</releaseDate>
